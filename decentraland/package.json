{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "SDK7 Template scene",
  "scripts": {
    "start": "sdk-commands start -p 8050 --host 0.0.0.0",
    "start:external": "sdk-commands start -p 8050 --host 0.0.0.0 --ci --no-browser",
    "start:remote": "./start-remote.sh",
    "start:tunnel": "sdk-commands start -p 8050 --host localhost",
    "deploy": "sdk-commands deploy -p 8050",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "@dcl/js-runtime": "7.8.21",
    "@dcl/sdk": "^7.8.21",
    "@noble/hashes": "^1.8.0",
    "@types/node": "^22.13.1",
    "@types/sha.js": "^2.4.4",
    "sha.js": "^2.4.12"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@dcl-sdk/utils": "^1.2.8",
    "@dcl/asset-packs": "^2.1.2",
    "@noble/hashes": "^1.8.8",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/web3.js": "^1.95.2",
    "bs58": "^5.0.0",
    "dcl-npc-toolkit": "^1.5.0",
    "eth-connect": "^6.2.4",
    "openmeteo": "^1.1.4",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  }
}